{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug executable 'omnect-ui'",
      "cargo": {
        "args": [
          "build",
          "--bin=omnect-ui",
          "--package=omnect-ui",
          "--features=mock"
        ],
        "filter": {
          "name": "omnect-ui",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}",
      "preLaunchTask": "check_ods_and_centrifugo",
      "env": {
        "RUST_LOG": "omnect_ui=debug",
        "SOCKET_PATH": "/tmp/api.sock",
        "CERT_PATH": "temp/device_id_cert.pem",
        "KEY_PATH": "temp/device_id_cert_key.pem",
        "CENTRIFUGO_ADMIN_ENABLED": "true",
        "CENTRIFUGO_ADMIN_PASSWORD": "123",
        "CENTRIFUGO_ADMIN_SECRET": "123",
        "CENTRIFUGO_LOG_LEVEL": "debug",
        "KEYCLOAK_URL": "https://keycloak.omnect.conplement.cloud/realms/cp-dev",
        "UI_PORT": "1977"
      }
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in executable 'omnect-ui'",
      "cargo": {
        "args": [
          "test",
          "--no-run",
          "--bin=omnect-ui",
          "--package=omnect-ui"
        ],
        "filter": {
          "name": "omnect-ui",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    }
  ]
}